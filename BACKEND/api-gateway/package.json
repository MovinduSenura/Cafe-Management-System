{
    "name": "cafe-api-gateway",
    "version": "1.0.0",
    "description": "API Gateway for Cafe Management Microservices",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "lint": "eslint . --ext .js --fix",
        "lint:check": "eslint . --ext .js",
        "docker:build": "docker build -t cafe-api-gateway .",
        "docker:run": "docker run -p 3000:3000 cafe-api-gateway"
    },
    "dependencies": {
        "express": "^4.21.1",
        "http-proxy-middleware": "^2.0.6",
        "express-rate-limit": "^7.4.0",
        "helmet": "^7.1.0",
        "cors": "^2.8.5",
        "jsonwebtoken": "^9.0.2",
        "redis": "^4.6.13",
        "winston": "^3.14.2",
        "dotenv": "^16.4.5",
        "opossum": "^8.0.1"
    },
    "devDependencies": {
        "nodemon": "^3.1.7",
        "jest": "^29.7.0",
        "supertest": "^6.3.4",
        "eslint": "^8.57.0",
        "@eslint/js": "^9.12.0"
    },
    "keywords": [
        "api-gateway",
        "microservices",
        "node.js",
        "kubernetes"
    ],
    "author": "Cafe Management Team",
    "license": "ISC"
}